{
  "manifest_version": 3,
  "name": "Gooofy",
  "version": "1.0.0",
  "description": "Auto-group tabs using local LLM semantic data",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
    "background": {
    "service_worker": "background.js"
  }
}
